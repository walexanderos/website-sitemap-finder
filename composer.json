{
    "name": "webignition/website-sitemap-finder",
    "description": "Finds the sitemap(.xml) for a given website",
    "keywords": ["sitemap"],
    "homepage": "https://github.com/webignition/website-sitemap-finder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "webignition\\Tests": "tests/",
            "": "src/"
        }
    },    
    "require": {
        "php": ">=5.4.0",
        "webignition/url":">=0.1,<2.0",
        "webignition/robots-txt-parser":">=1.0.2,<2.0",
        "webignition/internet-media-type":">=0.1,<2.0",
        "webignition/sitemap-model":">=1.0.4,<2.0",
        "webignition/website-sitemap-retriever":">=1.0.1,<2.0",
        "guzzle/guzzle":">=3.8.1",
        "webignition/absolute-url-deriver":">=1.3.3,<2.0",
        "symfony/event-dispatcher":">=2.1",
        "webignition/cookie-url-matcher":">=0.1,<1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "minimum-stability":"stable",
    "prefer-stable":true
}